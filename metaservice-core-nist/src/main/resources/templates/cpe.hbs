<div class="row">
    <div class="col-md-12">
        {{> button_controls}}
        <h1 {{provenance . "rdfs:label"}}>{{text rdfs:label}}</h1>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <ol class="breadcrumb">
            <li>Common Platform Enumeration</li>
            <li class="active">{{text cpe:name}}</li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <h1>{{text cpe:name}}</h1>
    </div>
</div>
<div class="row">
    <div class="col-md-8">
        <h2>Referenced By</h2>
        <ul>
            {{#eachArray dcterms:isReferencedBy}}
                <li> <a href="{{uri this}}">{{text rdfs:label}}</a> </li>
            {{/eachArray}}
        </ul>
    </div>
    <div class="col-md-8">
        <h2>Linked Releases</h2>
        <ul>
            {{#eachArray cpe:isAbout}}
                <li> <a href="{{uri this}}">{{text rdfs:label}}</a> </li>
            {{/eachArray}}
        </ul>
    </div>
</div>
