<div class="row">
    <div class="col-md-12">
        {{> button_controls}}
        <h1 data-provenance="[{{ms:id}}][dc:title][{{dc:title}}]">{{text dc:title}}</h1>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <ol class="breadcrumb">
            <li>Common Platform Enumeration</li>
            <li class="active">{{text dc:title}}</li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-md-8">
        <h2>Referenced By</h2>
        <ul>
            {{#eachArray dcterms:isReferencedBy}}
                <li> <a href="{{uri this}}">{{text dc:title}}</a> </li>
            {{/eachArray}}
        </ul>
    </div>
    <div class="col-md-8">
        <h2>Linked Releases</h2>
        <ul>
            {{#eachArray cpe:isAbout}}
                <li> <a href="{{uri this}}">{{text dc:title}}</a> </li>
            {{/eachArray}}
        </ul>
    </div>
</div>
