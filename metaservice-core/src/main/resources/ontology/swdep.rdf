    <!-- http://metaservice.org/ns/swdep#SoftwareDependency_Relation -->

    <owl:Class rdf:about="&swdep;SoftwareDependency_Relation">
        <rdfs:label xml:lang="en">SoftwareDependency_Relation</rdfs:label>
        <rdfs:subClassOf rdf:resource="&swdep;Dependency_Relation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&swdep;dependOn"/>
                <owl:someValuesFrom rdf:resource="&swdep;SW"/>
                <owl:someValuesFrom rdf:resource="&swdep;AnyOneOfSW"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>
            The most generic Software Dependency Relation.
            Its basic properties are &quot;dependencyOf&quot; and &quot;dependOn&quot;. The &quot;dependencyOf&quot; instance has some kind of dependency to the &quot;dependOn&quot; object.
            Since Dependency_Relation is very generic usually there is a better fitting subclass to declare dependencies.
        </dc:description>
        <rdfs:comment xml:lang="en">A dependency to a piece of Software</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://metaservice.org/ns/swdep#"/>
    </owl:Class>

