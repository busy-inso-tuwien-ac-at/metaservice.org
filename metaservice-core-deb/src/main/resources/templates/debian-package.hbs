<ol class="breadcrumb">
    <li><a href="#">Debian Binary Packages</a></li>
    <li><a href="{{admssw:release/admssw:project}}">{{dc:title}}</a></li>
    <li><a href="{{admssw:release/ms:id}}">{{ms-deb:version}}</a></li>
    <li class="active">{{ms-deb:architecture}}</li>
</ol>
{{> button_controls}}
<h1 data-provenance="[{{ms:id}}][dc:title][{{dc:title}}]">{{dc:title}}</h1>
<div class="row">
    <div class="col-md-8">
        <pre data-provenance="[{{ms:id}}][{ms-deb:description][{{ms-deb:description}}]">{{ms-deb:description}}</pre>
        Homegpage: {{#ms-deb:homepage}} <a class="external" href="{{this}}">{{this}}</a>{{/ms-deb:homepage}}{{^ms-deb:homepage}}-{{/ms-deb:homepage}}
        <br>
        Maintainer:
        <ul>
            {{#eachArray ms-deb:maintainer}}<li><a href="{{foaf:mbox}}">{{foaf:name}} &lt;{{foaf:mbox}}&gt;</a></li>{{/eachArray}}
        </ul>
        <br>
        Uploader:
        <ul>
            {{#eachArray ms-deb:uploader}}<li><a href="{{foaf:mbox}}">{{foaf:name}}</a></li>{{/eachArray}}
        </ul>
        Depends:
        <ul>
            {{#eachArray ms-deb:depends}}
                <li>
                {{#if dc:description}}
                    <a href="{{ms-deb:depends}}">{{dc:description}}</a>
                {{/if}}
                {{#eachArray rdf:li}}
                    <a href="{{ms-deb:depends}}">{{dc:description}}</a>
                    <b>or</b>
                {{/eachArray}}
            </li>
            {{/eachArray}}
        </ul>
        Suggests:
        <ul>
            {{#eachArray ms-deb:suggests}}<li><a href="{{ms-deb:suggests}}">{{dc:description}}</a>{{#eachArray rdf:li}}{{/eachArray}}</li>{{/eachArray}}
        </ul>
        Recommends:
        <ul>
            {{#eachArray ms-deb:recommends}}<li><a href="{{ms-deb:recommends}}">{{dc:description}}</a></li>{{/eachArray}}
        </ul>
    </div>
    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading"><h4 class="panel-title">Navigation</h4></div>
            <table class="table">
                <tr data-provenance="[{{ms:id}}][xhv:prev][{{xhv:prev/ms:id}}]"><th>Previous</th><td><a href="{{xhv:prev/ms:id}}">{{xhv:prev/ms-deb:version}}</a></td></tr>
                <tr data-provenance="[{{ms:id}}][xhv:next][{{xhv:next/ms:id}}]"><th>Next</th><td><a href="{{xhv:next/ms:id}}">{{xhv:next/ms-deb:version}}</a></td></tr>
                <tr><th>Version</th><td><a href="{{admssw:release/ms:id}}">{{ms-deb:version}}</a></td></tr>
                <tr><th>Project</th><td><a href="{{admssw:release/admssw:project}}">{{dc:title}}</a></td></tr>
                <tr><th>Repository</th><td></td></tr>
            </table>
        </div>

        <div class="panel panel-default">
            <div class="panel-heading"><h4 class="panel-title">File Information</h4></div>
            <table class="table">
                <tr><th>Filename</th><td>{{> copyInputField  ms-deb:filename}}</td></tr>
                <tr><th>MD5sum</th><td>{{> copyInputField ms-deb:md5sum}}</td></tr>
                <tr><th>SHA-1sum</th><td>{{> copyInputField  ms-deb:sha1sum}}</td></tr>
                <tr><th>SHA-256sum</th><td>{{> copyInputField  ms-deb:sha256sum}}</td></tr>
            </table>
        </div>
    </div>
</div>
